{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azure-ws"
		},
		"LS_ADLS_MetaData_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_MetaData'"
		},
		"LS_ADLS_eystgaccnt_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_eystgaccnt'"
		},
		"LS_AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSqlDatabase'"
		},
		"LS_Completed_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Completed'"
		},
		"LS_Assignment_csv_txt_files_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Assignment_csv_txt_files'"
		},
		"LS_ADLS_MetaData_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eystgaccnt.dfs.core.windows.net/"
		},
		"LS_ADLS_eystgaccnt_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eystgaccnt.dfs.core.windows.net/"
		},
		"LS_Completed_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eystgaccnt.dfs.core.windows.net/"
		},
		"LS_Assignment_csv_txt_files_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eystgaccnt.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azure-ws_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_MetaData_accountKey": {
						"value": "[parameters('LS_ADLS_MetaData_accountKey')]"
					},
					"LS_ADLS_eystgaccnt_accountKey": {
						"value": "[parameters('LS_ADLS_eystgaccnt_accountKey')]"
					},
					"LS_AzureSqlDatabase_connectionString": {
						"value": "[parameters('LS_AzureSqlDatabase_connectionString')]"
					},
					"LS_Completed_accountKey": {
						"value": "[parameters('LS_Completed_accountKey')]"
					},
					"LS_Assignment_csv_txt_files_accountKey": {
						"value": "[parameters('LS_Assignment_csv_txt_files_accountKey')]"
					},
					"LS_ADLS_MetaData_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_MetaData_properties_typeProperties_url')]"
					},
					"LS_ADLS_eystgaccnt_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_eystgaccnt_properties_typeProperties_url')]"
					},
					"LS_Completed_properties_typeProperties_url": {
						"value": "[parameters('LS_Completed_properties_typeProperties_url')]"
					},
					"LS_Assignment_csv_txt_files_properties_typeProperties_url": {
						"value": "[parameters('LS_Assignment_csv_txt_files_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "azure-ws_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'azure-ws_ArmTemplate_0')]"
			]
		}
	]
}